<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shot_tracker_app">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.shottracker.ShotTrackerApplication"
        android:theme="@style/Theme.Shottrackerapp">
        <activity
            android:name="com.shottracker.feature.home.ui.HomeActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Shottrackerapp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>